{
  "name": "ulbimiddle",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "start": "webpack serve --env port=3000",
    "start:vite": "vite --open",
    "start:dev:server": "node ./json-server/index.js",
    "start:dev": "concurrently \"webpack serve --env port=3000\" \"node ./json-server/index.js\"",
    "start:vite:dev": "concurrently \"npm run start:vite\" \"node ./json-server/index.js\"",
    "build:prod": "webpack --env mode=production baseApiUrl=https://ulbi-json-server.vercel.app",
    "build:dev": "webpack --env mode=development",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx}\"",
    "project:beauty": "npm run \"lint:ts:fix\" & npm run \"lint:scss:fix\" &  npm run \"prettier:fix\"",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:loki": "npx loki test --port 6006",
    "test:loki:ok": "npx loki approve",
    "test:loki:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "test:loki:json": "node scripts/generate-visual-json-report.js",
    "test:loki:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:loki:report": "npm run test:loki:json & npm run test:loki:html",
    "sb": "start-storybook -p 6006 -c ./config/storybook",
    "sb:build": "build-storybook -c ./config/storybook",
    "slice": "node ./scripts/createFSDSlice/index.js",
    "prepare": "husky install"
  },
